{
  "name": "lock-tree",
  "displayName": "Lock Tree",
  "description": "Visualize the Lock Dependency Tree",
  "version": "0.0.1",
  "icon": "icon.png",
  "main": "./out/extension.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/manuartero/lock-tree"
  },
  "engines": {
    "vscode": "^1.76.0"
  },
  "license": "LICENSE",
  "categories": [
    "Visualization"
  ],
  "author": {
    "name": "Manuel Artero",
    "email": "manutero.developer@gmail.com"
  },
  "capabilities": {
    "customEditors": true
  },
  "activationEvents": [
    "onView:viewId.lock-tree.editor.view"
  ],
  "contributes": {
    "customEditors": [
      {
        "viewType": "lock-tree.editor.view",
        "displayName": "Lock Tree",
        "selector": [
          {
            "filenamePattern": "*-lock.json"
          }
        ]
      }
    ],
    "commands": [
      {
        "command": "lock-tree.command.open-lock",
        "title": "Open Lock File"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.76.0",
    "@types/vscode-webview": "^1.57.1",
    "@typescript-eslint/eslint-plugin": "^5.53.0",
    "@typescript-eslint/parser": "^5.53.0",
    "@vscode/test-electron": "^2.2.3",
    "eslint": "^8.34.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^4.9.5"
  }
}
